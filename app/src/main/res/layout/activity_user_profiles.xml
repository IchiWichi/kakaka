<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_user_profiles"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="bonaguasato.location.User_profiles">

    <ImageButton
        android:scaleType="fitCenter"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/owner"
        android:layout_marginLeft="29dp"
        android:layout_marginStart="29dp"
        android:id="@+id/imageButtonowner"
        android:adjustViewBounds="true"
        android:cropToPadding="false"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="179dp"
        tools:ignore="ContentDescription,RtlHardcoded" />

    <TextView
        android:text="Owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        android:layout_below="@+id/imageButtonguest"
        android:layout_alignRight="@+id/imageButtonowner"
        android:layout_alignEnd="@+id/imageButtonowner"
        android:layout_marginRight="23dp"
        android:layout_marginEnd="23dp"
        android:layout_marginTop="15dp"
        android:typeface="normal"
        android:textColor="@android:color/background_dark"
        android:textSize="18sp"
        tools:ignore="HardcodedText,RtlHardcoded" />

    <TextView
        android:text="Guest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView"
        android:layout_alignLeft="@+id/imageButtonguest"
        android:layout_alignStart="@+id/imageButtonguest"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp"
        android:id="@+id/textView5"
        android:textSize="18sp"
        android:textColor="@android:color/background_dark"
        tools:ignore="HardcodedText,RtlHardcoded"
        android:layout_alignTop="@+id/textView" />

    <ImageButton
        android:scaleType="fitCenter"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/guest"
        android:layout_marginRight="39dp"
        android:layout_marginEnd="56dp"
        android:id="@+id/imageButtonguest"
        android:contentDescription=""
        tools:ignore="ContentDescription,RtlHardcoded"
        android:layout_alignTop="@+id/imageButtonowner"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
