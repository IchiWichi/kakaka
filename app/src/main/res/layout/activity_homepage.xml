<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_homepage"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="bonaguasato.location.Homepage">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#616161"
        tools:ignore="UselessParent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#FCE4EC"
            android:layout_alignParentTop="true"
            android:layout_marginTop="50dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            tools:ignore="RtlHardcoded">


            <ImageButton
                android:background="@drawable/users"
                android:layout_width="100dip"
                android:layout_height="110dip"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginBottom="367dp"
                android:id="@+id/imgbtnUser"
                android:soundEffectsEnabled="false"
                tools:soundEffectsEnabled="false"
                tools:ignore="ContentDescription" />

            <ImageButton
                android:background="@drawable/web"
                android:layout_width="100dip"
                android:layout_height="90dip"
                android:id="@+id/imgbtnbehavior"
                tools:ignore="ContentDescription"
                android:layout_alignTop="@+id/imgbtnUser"
                android:layout_centerHorizontal="true" />

            <ImageButton
                android:background="@drawable/eye"
                android:layout_width="100dip"
                android:layout_height="60dip"
                android:adjustViewBounds="true"
                tools:ignore="ContentDescription"
                android:layout_alignBottom="@+id/imgbtnbehavior"
                android:layout_toEndOf="@+id/imgbtnbehavior"
                android:layout_marginBottom="9dp"
                android:id="@+id/imgbtncamera" />

            <TextView
                android:text="Camera"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="false"
                android:layout_marginRight="14dp"
                android:layout_marginEnd="14dp"
                android:layout_marginBottom="360dp"
                android:id="@+id/textView7"
                tools:textColor="@android:color/holo_blue_dark"
                android:layout_alignParentBottom="true"
                android:layout_below="@+id/imageButton13"
                android:textStyle="normal|bold"
                android:textAllCaps="true"
                android:textColor="@android:color/holo_blue_dark"
                tools:ignore="HardcodedText,UnknownId" />

            <ImageButton
                android:background="@drawable/location"
                android:layout_width="70dip"
                android:layout_height="60dip"
                android:adjustViewBounds="true"
                tools:ignore="ContentDescription"
                android:id="@+id/imgbtnlocation"
                android:layout_alignTop="@+id/textView2"
                android:layout_alignEnd="@+id/imgbtnUser"
                android:layout_marginTop="48dp" />

            <ImageButton
                android:background="@drawable/scan"
                android:layout_width="100dip"
                android:layout_height="100dip"
                android:adjustViewBounds="true"
                tools:ignore="ContentDescription"
                android:id="@+id/imgbtnscan"
                android:layout_marginTop="35dp"
                android:layout_alignTop="@+id/textView7"
                android:layout_alignEnd="@+id/textView7" />

            <TextView
                android:text="Location"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignParentStart="true"
                android:layout_marginStart="37dp"
                android:layout_marginBottom="233dp"
                android:textColor="@android:color/holo_blue_dark"
                android:textStyle="normal|bold"
                android:textAllCaps="true"
                tools:ignore="HardcodedText,UnknownId"
                android:id="@+id/textView8" />

            <TextView
                android:text="Text Scan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="40dp"
                android:layout_marginBottom="223dp"
                android:textColor="@android:color/holo_blue_dark"
                android:textStyle="normal|bold"
                android:textAllCaps="true"
                tools:ignore="HardcodedText,UnknownId"
                android:id="@+id/textView9" />

            <TextView
                android:text="BEHAVIOR VIEWER"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView6"
                tools:textColor="@android:color/holo_blue_dark"
                android:textAllCaps="true"
                android:textStyle="normal|bold"
                android:textColor="@android:color/holo_blue_dark"
                tools:ignore="HardcodedText"
                android:layout_below="@+id/imgbtnbehavior"
                android:layout_alignStart="@+id/imgbtnbehavior" />

            <TextView
                android:text="User Profile"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView2"
                tools:textColor="@android:color/holo_blue_dark"
                android:textStyle="normal|bold"
                android:textAllCaps="true"
                android:textColor="@android:color/holo_blue_dark"
                tools:ignore="HardcodedText"
                android:layout_alignBaseline="@+id/textView6"
                android:layout_alignBottom="@+id/textView6"
                android:layout_alignEnd="@+id/imgbtnlocation" />


        </RelativeLayout>

        <ImageView
            android:layout_width="50dip"
            android:layout_height="50dip"
            app:srcCompat="@drawable/logo"
            android:id="@+id/imageView"
            android:adjustViewBounds="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            tools:ignore="ContentDescription,RtlHardcoded" />

    </RelativeLayout>
</RelativeLayout>
